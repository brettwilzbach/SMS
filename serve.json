{
  "public": "dist",
  "single": true,
  "headers": [
    { 
      "source": "**/*.html",
      "headers": [{ 
        "key": "Cache-Control", 
        "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0" 
      }]
    },
    { 
      "source": "**/*.js",
      "headers": [{ 
        "key": "Cache-Control", 
        "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0" 
      }]
    },
    { 
      "source": "**/*.css",
      "headers": [{ 
        "key": "Cache-Control", 
        "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0" 
      }]
    }
  ]
}
